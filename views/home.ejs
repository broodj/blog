<%- include('partials/header') -%>

  <h1>Home</h1>
  <p><%= homeContent %></p>

  <% newBlogPost.forEach(function(blogPost){ %>
      <h1> <%= blogPost.postTitle %> </h1>
      <p>
        <%= blogPost.postBody.substr(0,100) + " ..." %>
        <a href="/posts/<%= blogPost.postTitle %>">Read More</a>
      </p>

  <% }); %>

<%- include('partials/footer') -%>
